<h1>Listing learning_solutions</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @learning_solutions.each do |learning_solution| %>
      <tr>
        <td><%= learning_solution.description %></td>
        <td><%= learning_solution.category %></td>
        <td><%= link_to 'Show', learning_solution %></td>
        <td><%= link_to 'Edit', edit_learning_solution_path(learning_solution) %></td>
        <td><%= link_to 'Destroy', learning_solution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Learning solution', new_learning_solution_path %>
